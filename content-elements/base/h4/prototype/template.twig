{% macro render(title = null, elementId = null, titlePartId = null) %}{% apply spaceless %}
    {% set title = title ?: bsi_cx_lorem(3) %}
    {% set elementId = elementId ?: 'title-h4-CNboRc' %}
    {% set titlePartId = titlePartId ?: 'title-h4-part-2TWXBi' %}
    <h4 data-bsi-element="{{ elementId }}" data-bsi-element-part="{{ titlePartId }}" class="{{ 'bsi-element-' ~ elementId }} bsi-heading-element bsi-text-primary-color bsi-default">{{ title }}</h4>
{% endapply %}{% endmacro %}