{% macro render(title = null, elementId = null, titlePartId = null) %}{% apply spaceless %}
    {% set title = title ?: bsi_cx_lorem(3) %}
    {% set elementId = elementId ?: 'title-h5-W7GGAn' %}
    {% set titlePartId = titlePartId ?: 'title-h5-part-U3qcnw' %}
    <h5 data-bsi-element="{{ elementId }}" data-bsi-element-part="{{ titlePartId }}" class="{{ 'bsi-element-' ~ elementId }} bsi-heading-element bsi-text-primary-color bsi-default">{{ title }}</h5>
{% endapply %}{% endmacro %}