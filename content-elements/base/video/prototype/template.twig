{% macro render(elementId = null, elementPartId = null) %}{% apply spaceless %}
    {% set elementId = elementId ?: 'video-C4cbKm' %}
    {% set elementPartId = elementPartId ?: 'video-part-loPxhG' %}

    <div data-bsi-element="{{ elementId }}" class="video {{ 'bsi-element-' ~ elementId }} mb-3">
        <div class="responsive-video" data-bsi-element-part="{{ elementPartId }}" data-bsi-video-data-link="https://www.youtube.com/embed/Ekc_4btc-iM">
            <iframe width="1120" height="630" src="{{ src ?: "https://www.youtube.com/embed/Ekc_4btc-iM" }}" allow="autoplay; encrypted-media" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
        </div>
    </div>

{% endapply %}{% endmacro %}